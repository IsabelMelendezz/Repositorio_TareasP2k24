Algoritmo cuad_cub_Num
	Escribir '--------Bienvenido----------';
	Escribir 'Ingrese el numero que desea elevar:';
	Leer n;
	cuad <- n^2;
	cub <- n^3;
	// Forma 2--------------
	// cuad=n*n;
	// cub=n*n*n;
	Escribir 'El cuadrado del numero ', n, ' es: ', cuad;
	Escribir 'El cubo del numero ', n, ' es: ', cub;
FinAlgoritmo
